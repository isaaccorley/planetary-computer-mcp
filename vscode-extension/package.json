{
  "name": "planetary-computer-mcp",
  "displayName": "Planetary Computer STAC MCP",
  "version": "0.1.0",
  "publisher": "isaaccorley",
  "description": "VS Code extension to configure the Planetary Computer STAC MCP server",
  "author": {
    "name": "Isaac Corley"
  },
  "categories": [
    "AI",
    "Machine Learning",
    "Other"
  ],
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#C80000",
    "theme": "dark"
  },
  "pricing": "Free",
  "activationEvents": [
    "onStartupFinished"
  ],
  "engines": {
    "vscode": "^1.100.0"
  },
  "main": "./dist/extension.js",
  "contributes": {
    "mcpServerDefinitionProviders": [
      {
        "id": "planetary-computer-mcp",
        "label": "Planetary Computer MCP Server"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -p ./ -w",
    "lint": "bunx eslint . --ext .ts,.tsx,.js --fix",
    "format": "bunx prettier --write .",
    "check:prettier": "bunx prettier --check .",
    "typecheck": "bunx tsc --noEmit",
    "check": "bun run typecheck && bun run check:prettier && bun run lint",
    "prepack": "bun run compile",
    "package": "bunx @vscode/vsce package",
    "publish": "bunx @vscode/vsce publish --pat $VSCE_TOKEN"
  },
  "dependencies": {},
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/node": "^18.0.0",
    "typescript": "^5.0.0",
    "@types/vscode": "^1.60.0",
    "@vscode/vsce": "^3.7.1",
    "typescript-eslint": "^8.50.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "bun-types": "^1.3.5",
    "eslint": "^9.39.2"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/isaaccorley/planetary-computer-mcp/issues",
    "email": "isaac.corley@proton.me"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/isaaccorley/planetary-computer-mcp.git"
  },
  "homepage": "https://github.com/isaaccorley/planetary-computer-mcp/blob/main/README.md"
}
